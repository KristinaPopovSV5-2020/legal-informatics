<?xml version="1.0" encoding="UTF-8"?>
<lrml:LegalRuleML xmlns:lrml="http://docs.oasis-open.org/legalruleml/ns/v1.0/"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://docs.oasis-open.org/legalruleml/ns/v1.0/ xsd-compact/lrml-compact.xsd"
	xmlns:ruleml="http://ruleml.org/spec">
	<lrml:Statements>
		<lrml:PrescriptiveStatement key="ps9">
			<ruleml:Rule key=":rule9" closure="universal" strength="defeasible">
				<ruleml:if>
					<ruleml:Atom>
						<ruleml:Rel iri="lc:low_income" />
						<ruleml:Var type="lc:defendant">Defendant</ruleml:Var>
						<ruleml:Data xsi:type="xs:string">yes</ruleml:Data>
					</ruleml:Atom>
				</ruleml:if>
				<ruleml:then>
					<ruleml:Atom>
						<ruleml:Rel>recommend_fine_reduction</ruleml:Rel>
						<ruleml:Var type="lc:defendant">Defendant</ruleml:Var>
					</ruleml:Atom>
				</ruleml:then>
			</ruleml:Rule>
		</lrml:PrescriptiveStatement>

		<lrml:PrescriptiveStatement key="ps10">
			<ruleml:Rule key=":rule10" closure="universal" strength="defeasible">
				<ruleml:if>
					<ruleml:Atom>
						<ruleml:Rel iri="lc:previously_convicted" />
						<ruleml:Var type="lc:defendant">Defendant</ruleml:Var>
						<ruleml:Data xsi:type="xs:string">yes</ruleml:Data>
					</ruleml:Atom>
				</ruleml:if>
				<ruleml:then>
					<ruleml:Atom>
						<ruleml:Rel>recommend_increased_penalty</ruleml:Rel>
						<ruleml:Var type="lc:defendant">Defendant</ruleml:Var>
					</ruleml:Atom>
				</ruleml:then>
			</ruleml:Rule>
		</lrml:PrescriptiveStatement>

		<lrml:PrescriptiveStatement key="ps11">
			<ruleml:Rule key=":rule11" closure="universal" strength="defeasible">
				<ruleml:if>
					<ruleml:And>
						<ruleml:Or>
							<ruleml:Atom>
								<ruleml:Rel iri="lc:weapon_type" />
								<ruleml:Var type="lc:defendant">Defendant</ruleml:Var>
								<ruleml:Data xsi:type="xs:string">B</ruleml:Data>
							</ruleml:Atom>
							<ruleml:Atom>
								<ruleml:Rel iri="lc:weapon_type" />
								<ruleml:Var type="lc:defendant">Defendant</ruleml:Var>
								<ruleml:Data xsi:type="xs:string">C</ruleml:Data>
							</ruleml:Atom>
							<ruleml:Atom>
								<ruleml:Rel iri="lc:weapon_type" />
								<ruleml:Var type="lc:defendant">Defendant</ruleml:Var>
								<ruleml:Data xsi:type="xs:string">D</ruleml:Data>
							</ruleml:Atom>
						</ruleml:Or>
						<ruleml:Atom>
							<ruleml:Rel iri="lc:method_of_weapon_discovery" />
							<ruleml:Var type="lc:defendant">Defendant</ruleml:Var>
							<ruleml:Data xsi:type="xs:string">public</ruleml:Data>
						</ruleml:Atom>
					</ruleml:And>
				</ruleml:if>
				<ruleml:then>
					<ruleml:Atom>
						<ruleml:Rel>is_high_category_weapon_in_public</ruleml:Rel>
						<ruleml:Var type=":defendant">Defendant</ruleml:Var>
					</ruleml:Atom>
				</ruleml:then>
			</ruleml:Rule>
		</lrml:PrescriptiveStatement>

		<lrml:PenaltyStatement key="pen10">
			<lrml:SuborderList>
				<lrml:Obligation>
					<ruleml:Atom>
						<ruleml:Rel iri=":to_pay_less" />
						<ruleml:Var>Suggestion</ruleml:Var>
						<ruleml:Ind>1</ruleml:Ind>
					</ruleml:Atom>

				</lrml:Obligation>
			</lrml:SuborderList>
		</lrml:PenaltyStatement>

		<lrml:PenaltyStatement key="pen11">
			<lrml:SuborderList>
				<lrml:Obligation>
					<ruleml:Atom>
						<ruleml:Rel iri=":to_increase_penalty" />
						<ruleml:Var>Suggestion</ruleml:Var>
						<ruleml:Ind>1</ruleml:Ind>
					</ruleml:Atom>

				</lrml:Obligation>
			</lrml:SuborderList>
		</lrml:PenaltyStatement>
		<lrml:PenaltyStatement key="pen12">
			<lrml:SuborderList>
				<lrml:Obligation>
					<ruleml:Atom>
						<ruleml:Rel iri=":to_pay_max" />
						<ruleml:Var>Money</ruleml:Var>
						<ruleml:Ind>500</ruleml:Ind>
					</ruleml:Atom>

				</lrml:Obligation>
			</lrml:SuborderList>
		</lrml:PenaltyStatement>
		<lrml:PenaltyStatement key="pen13">
			<lrml:SuborderList>
				<lrml:Obligation>
					<ruleml:Atom>
						<ruleml:Rel iri=":to_pay_min" />
						<ruleml:Var>Money</ruleml:Var>
						<ruleml:Ind>30</ruleml:Ind>
					</ruleml:Atom>

				</lrml:Obligation>
			</lrml:SuborderList>
		</lrml:PenaltyStatement>


		<lrml:ReparationStatement>
			<lrml:Reparation>
				<lrml:appliesPenalty keyref="#pen10" />
				<lrml:toPrescriptiveStatement keyref="#ps9" />
			</lrml:Reparation>
		</lrml:ReparationStatement>
		<lrml:ReparationStatement>
			<lrml:Reparation>
				<lrml:appliesPenalty keyref="#pen11" />
				<lrml:toPrescriptiveStatement keyref="#ps10" />
			</lrml:Reparation>
		</lrml:ReparationStatement>
		<lrml:ReparationStatement>
			<lrml:Reparation>
				<lrml:appliesPenalty keyref="#pen12" />
				<lrml:toPrescriptiveStatement keyref="#ps11" />
			</lrml:Reparation>
		</lrml:ReparationStatement>
		<lrml:ReparationStatement>
			<lrml:Reparation>
				<lrml:appliesPenalty keyref="#pen13" />
				<lrml:toPrescriptiveStatement keyref="#ps11" />
			</lrml:Reparation>
		</lrml:ReparationStatement>

	</lrml:Statements>
</lrml:LegalRuleML>